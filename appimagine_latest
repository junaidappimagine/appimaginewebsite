 <script>
 
 document.ready = function() {            
        document.getElementById("service").style.visibility = "hidden"; 
		 document.getElementById("sub").style.visibility = "hidden"; 
		 
    }
        function iconChat(){
            $('#chatImg').hide();
            document.getElementById('newChat').style.visibility="visible";
            window.location="http://www.appimagine.com/contact-us";
        }
		function hideElem() {
      document.getElementById("sub").style.visibility = "hidden"; 
	  document.getElementById("service").style.visibility = "hidden"; 
}

function showElem() {
    document.getElementById("sub").style.visibility = "visible"; 
	  document.getElementById("service").style.visibility = "hidden"; 
}
function hideElem1() {
    document.getElementById("service").style.visibility = "hidden"; 
}

function showElem1() {
    document.getElementById("service").style.visibility = "visible"; 
}
		
		
    </script>